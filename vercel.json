{
  "buildCommand": "echo 'Static site'",
  "outputDirectory": ".",
  "framework": null,
  "headers": [
    {
      "source": "/(.*\\.pdf)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!docs|assets|data|registros|.*\\.html|.*\\.pdf|.*\\.json|.*\\.js|.*\\.css).*)",
      "destination": "/index.html"
    }
  ]
}
