{
  "buildCommand": "echo 'Portal de Calidad ASCH-OHLA - UTE A11 - Vers√£o Final 25/09/2025'",
  "outputDirectory": ".",
  "framework": null,
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.pdf)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!docs|assets|data|registros|.*\\.html|.*\\.pdf|.*\\.json|.*\\.js|.*\\.css).*)",
      "destination": "/index.html"
    }
  ]
}
